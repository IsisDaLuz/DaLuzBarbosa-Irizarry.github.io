var currentScene = 0;
var startTime;
var score = 0;
//khan button
var Button = function(config) {
    this.x = config.x || 0;
    this.y = config.y || 0;
    this.width = config.width || 150;
    this.height = config.height || 50;
    this.label = config.label || "Click";
    //this.align = config.textAlign;
    this.onClick = config.onClick || function() {};
};

Button.prototype.draw = function() {
    fill(227, 170, 0);
    rect(this.x, this.y, this.width, this.height, 5);
    fill(0, 0, 0);
    textSize(18);
    textAlign(LEFT, TOP);
    text(this.label, this.x+10, this.y+this.height/4);
};

Button.prototype.isMouseInside = function() {
    return mouseX > this.x &&
           mouseX < (this.x + this.width) &&
           mouseY > this.y &&
           mouseY < (this.y + this.height);
};

Button.prototype.handleMouseClick = function() {
    if (this.isMouseInside()) {
        this.onClick();
    }
};

var drawHead = function (bitmojiX, bitmojiY, bitmojiHeight){
//head
noStroke();
fill(227, 164, 127);
ellipse (bitmojiX,bitmojiY,bitmojiHeight/150*86,bitmojiHeight/150*100); //head
fill(255, 184, 218);
ellipse(bitmojiX-(bitmojiHeight/150*23),bitmojiY+(bitmojiHeight/150*25),bitmojiHeight/150*12,bitmojiHeight/150*7);//cheeks
ellipse(bitmojiX+(bitmojiHeight/150*24),bitmojiY+(bitmojiHeight/150*25),bitmojiHeight/150*12,bitmojiHeight/150*7);
fill(255, 255, 255);



//earings
fill(250, 252, 141);
ellipse(bitmojiX-(bitmojiHeight/150*39),bitmojiY+(bitmojiHeight/150*20),(bitmojiHeight/150*4),(bitmojiHeight/150*7));
ellipse(bitmojiX+(bitmojiHeight/150*39),bitmojiY+(bitmojiHeight/150*20),bitmojiHeight/150*4,bitmojiHeight/150*7);
//hair
fill(61, 13, 13);

arc(bitmojiX, bitmojiY-(bitmojiHeight/150*18), (bitmojiHeight/150*94), bitmojiHeight/150*66, 157,380);//hair

ellipse(bitmojiX,bitmojiY-(bitmojiHeight/150*57),bitmojiHeight/150*45,bitmojiHeight/150*38);//hair bun
stroke(61, 13, 13);
strokeWeight(7);
line(bitmojiX-(bitmojiHeight/150*31), bitmojiY-(bitmojiHeight/150*17), bitmojiX+(bitmojiHeight/150*43), bitmojiY-(bitmojiHeight/150*15));//hair line
//headband
noStroke();
fill(255, 229, 254);
arc(bitmojiX, bitmojiY-(bitmojiHeight/150*26), bitmojiHeight/150*103, bitmojiHeight/150*17, 163,377);
//eye
noStroke();
fill(255, 255, 255);//eye
ellipse(bitmojiX-(bitmojiHeight/150*20),bitmojiY+(bitmojiHeight/150*9),bitmojiHeight/150*18,bitmojiHeight/150*12);//right eye
ellipse(bitmojiX+(bitmojiHeight/150*19),bitmojiY+(bitmojiHeight/150*9),bitmojiHeight/150*18,bitmojiHeight/150*12);//left eye
//eyebrows
strokeWeight(2);
stroke(59, 28, 2);
fill(227, 164, 127);
arc(bitmojiX-(bitmojiHeight/150*20),bitmojiY+(bitmojiHeight/150*4),(bitmojiHeight/150*24),(bitmojiHeight/150*-13),44,169);
arc(bitmojiX+(bitmojiHeight/150*19),bitmojiY+(bitmojiHeight/150*4),(bitmojiHeight/150*24),(bitmojiHeight/150*-12),28,138);
//eye
fill(54, 25, 2);
ellipse(bitmojiX-(bitmojiHeight/150*20),bitmojiY+(bitmojiHeight/150*9),bitmojiHeight/150*7,bitmojiHeight/150*7);//right eye pupil
ellipse(bitmojiX+(bitmojiHeight/150*19),bitmojiY+(bitmojiHeight/150*9),bitmojiHeight/150*7,bitmojiHeight/150*7);//left eye pupil
//nose
stroke(0,0,0); //nose stroke
strokeWeight(1);
fill(198, 136, 99);//skin tone
bezier(bitmojiX,bitmojiY+(bitmojiHeight/150*11),bitmojiX+(bitmojiHeight/150*22),bitmojiY+(bitmojiHeight/150*24),bitmojiX-(bitmojiHeight/150*3),bitmojiY+(bitmojiHeight/150*27),bitmojiX-(bitmojiHeight/150*3),bitmojiY+(bitmojiHeight/150*25)); // nose
//mouth
fill(255, 255, 255);//teeth
arc(bitmojiX+(bitmojiHeight/150*1),bitmojiY+(bitmojiHeight/150*32),bitmojiHeight/150*30,bitmojiHeight/150*13,1,180);//bottom mouth
line(bitmojiX-(bitmojiHeight/150*14),bitmojiY+(bitmojiHeight/150*33),bitmojiX+(bitmojiHeight/150*16),bitmojiY+(bitmojiHeight/150*32));//top mouth
};

var drawBody = function (bitmojiX, bitmojiY, bitmojiHeight){
//neck
noStroke();
fill(227, 164, 127);
rect(bitmojiX-(bitmojiHeight/150*13),bitmojiY+(bitmojiHeight/150*44),(bitmojiHeight/150*26),(bitmojiHeight/150*15));
//shirt
fill(0, 0, 0);
rect(bitmojiX-(bitmojiHeight/150*63), bitmojiY+(bitmojiHeight/150*56), bitmojiHeight/150*129, bitmojiHeight/150*58);//shirt
fill(227, 164, 127);
arc(bitmojiX+(bitmojiHeight/150*1),bitmojiY+(bitmojiHeight/150*55),bitmojiHeight/150*30,bitmojiHeight/150*23,1,180);// shirt arc
fill(252, 252, 252);
ellipse(bitmojiX-(bitmojiHeight/150*39),bitmojiY+(bitmojiHeight/150*72),bitmojiHeight/150*8,bitmojiHeight/150*8);
//textname
fill(255, 255, 255);

textSize(bitmojiHeight/150*30);

text("IDLB",bitmojiX-(bitmojiHeight/150*32),bitmojiY+(bitmojiHeight/150*85),(bitmojiHeight/150*211),bitmojiHeight/150*201);

//shirt details
fill(255, 148, 251);
stroke(255, 255, 255);
//flowerpetals
ellipse(bitmojiX-(bitmojiHeight/150*26),bitmojiY+(bitmojiHeight/150*72),bitmojiHeight/150*10,bitmojiHeight/150*9);
ellipse(bitmojiX-(bitmojiHeight/150*38),bitmojiY+(bitmojiHeight/150*72),bitmojiHeight/150*10,bitmojiHeight/150*9);
ellipse(bitmojiX-(bitmojiHeight/150*36),bitmojiY+(bitmojiHeight/150*78),bitmojiHeight/150*10,bitmojiHeight/150*9);
ellipse(bitmojiX-(bitmojiHeight/150*28),bitmojiY+(bitmojiHeight/150*78),bitmojiHeight/150*10,bitmojiHeight/150*9);
ellipse(bitmojiX-(bitmojiHeight/150*33),bitmojiY+(bitmojiHeight/150*67),bitmojiHeight/150*10,bitmojiHeight/150*9);
fill(184, 242, 205);//centerflower
ellipse(bitmojiX-(bitmojiHeight/150*32),bitmojiY+(bitmojiHeight/150*73),bitmojiHeight/150*7,bitmojiHeight/150*6);
};

var drawBitmojiIsis=function (bitmojiX, bitmojiY, bitmojiHeight){
    
    drawHead(bitmojiX, bitmojiY,  bitmojiHeight);
    drawBody(bitmojiX, bitmojiY, bitmojiHeight);
    
};

var drawhead= function(headX,headY,alanisHeight) {
   //Alanis
  
noStroke();
fill(224,172,105);
ellipse(headX+(100*alanisHeight/150),headY+(100*alanisHeight/150),83*alanisHeight/150,100*alanisHeight/150);//head

fill(255, 255, 255);
arc(headX+(58*alanisHeight/150), headY+(135*alanisHeight/150), -4*alanisHeight/150, 55*alanisHeight/150, 0*alanisHeight/150, 361*alanisHeight/150);//left cheek
arc(headX+(140*alanisHeight/150), headY+(128*alanisHeight/150), 8*alanisHeight/150, 55*alanisHeight/150, 0*alanisHeight/150, 361*alanisHeight/150);//right cheek

};

var drawhair= function(hairX,hairY,alanisHeight) {
  
fill(82, 31, 0);//ALL IS HAIR
quad(hairX+(142*alanisHeight/150),hairY+(107*alanisHeight/150),hairX+(79*alanisHeight/150),hairY+(58*alanisHeight/150),hairX+(113*alanisHeight/150),hairY+(60*alanisHeight/150),hairX+(133*alanisHeight/150),hairY+(105*alanisHeight/150));//right hair

quad(hairX+(57*alanisHeight/150),hairY+(107*alanisHeight/150),hairX+(62*alanisHeight/150),hairY+(68*alanisHeight/150),hairX+(107*alanisHeight/150),hairY+(43*alanisHeight/150),hairX+(74*alanisHeight/150),hairY+(105*alanisHeight/150));//right hair

ellipse(hairX+(110*alanisHeight/150),hairY+(65*alanisHeight/150),38*alanisHeight/150,23*alanisHeight/150);//right part

ellipse(hairX+(129*alanisHeight/150),hairY+(83*alanisHeight/150),14*alanisHeight/150,39*alanisHeight/150);//left part

ellipse(hairX+(133*alanisHeight/150),hairY+(90*alanisHeight/150),11*alanisHeight/150,47*alanisHeight/150);//left part

ellipse(hairX+(122*alanisHeight/150),hairY+(74*alanisHeight/150),11*alanisHeight/150,13*alanisHeight/150);//left part

rect(hairX+(59*alanisHeight/150),hairY+(101*alanisHeight/150),16*alanisHeight/150,98*alanisHeight/150,88*alanisHeight/150);//left hair longer

rect(hairX+(61*alanisHeight/150),hairY+(66*alanisHeight/150),16*alanisHeight/150,163*alanisHeight/150,88*alanisHeight/150);//double it

rect(hairX+(127*alanisHeight/150),hairY+(100*alanisHeight/150),16*alanisHeight/150,88*alanisHeight/150,88*alanisHeight/150);//right hair longer

rect(hairX+(127*alanisHeight/150),hairY+(100*alanisHeight/150),16*alanisHeight/150,88*alanisHeight/150,88*alanisHeight/150);//double it



//more hair
fill(110, 76, 58);
ellipse(hairX+(139*alanisHeight/150),hairY+(166*alanisHeight/150),16*alanisHeight/150,157*alanisHeight/150);
ellipse(hairX+(64*alanisHeight/150),hairY+(166*alanisHeight/150),16*alanisHeight/150,157*alanisHeight/150);

};

var drawfeatures= function(feaX,feaY,alanisHeight) {
//face
stroke(0, 0, 0);//face
fill(255, 255, 255);
ellipse(feaX+(86*alanisHeight/150),feaY+(97*alanisHeight/150),14*alanisHeight/150,5*alanisHeight/150);//eye
ellipse(feaX+(115*alanisHeight/150),feaY+(96*alanisHeight/150),14*alanisHeight/150,5*alanisHeight/150);//eye
fill(10, 9, 9);
ellipse(feaX+(86*alanisHeight/150),feaY+(96*alanisHeight/150),7*alanisHeight/150,6*alanisHeight/150);//eye
ellipse(feaX+(115*alanisHeight/150),feaY+(96*alanisHeight/150),7*alanisHeight/150,6*alanisHeight/150);//eye

rect(feaX+(72*alanisHeight/150),feaY+(87*alanisHeight/150),21*alanisHeight/150,2*alanisHeight/150);//right brow
rect(feaX+(108*alanisHeight/150),feaY+(87*alanisHeight/150),21*alanisHeight/150,2*alanisHeight/150);//left brow

fill(224,172,105);
bezier(feaX+(96*alanisHeight/150),feaY+(102*alanisHeight/150),feaX+(92*alanisHeight/150),feaY+(117*alanisHeight/150),feaX+(91*alanisHeight/150),feaY+(117*alanisHeight/150),feaX+(101*alanisHeight/150),feaY+(117*alanisHeight/150));//nose

noStroke();
fill(245, 112, 163);
arc(feaX+(100*alanisHeight/150),feaY+(129*alanisHeight/150),30*alanisHeight/150,13*alanisHeight/150,1*alanisHeight/150,180*alanisHeight/150);

};

var drawbody= function(bodyX,bodyY,alanisHeight) {
//body
fill(224,172,105);
rect(bodyX+(85*alanisHeight/150),bodyY+(148*alanisHeight/150),30*alanisHeight/150,10*alanisHeight/150);//neck
rect(bodyX+(70*alanisHeight/150),bodyY+(157*alanisHeight/150),60*alanisHeight/150,43*alanisHeight/150);//shoulders
fill(185, 147, 196);
rect(bodyX+(70*alanisHeight/150),bodyY+(164*alanisHeight/150),60*alanisHeight/150,43*alanisHeight/150);//dress
fill(201, 189, 189);
rect(bodyX+(86*alanisHeight/150),bodyY+(147*alanisHeight/150),4*alanisHeight/150,29*alanisHeight/150);//necklace right
rect(bodyX+(110*alanisHeight/150),bodyY+(147*alanisHeight/150),4*alanisHeight/150,28*alanisHeight/150);//necklace left
rect(bodyX+(88*alanisHeight/150),bodyY+(172*alanisHeight/150),24*alanisHeight/150,5*alanisHeight/150);//necklace center

//headwear
var HB=53;
var HBB=44;
ellipse(bodyX+(101*alanisHeight/150),bodyY+(56*alanisHeight/150),55*alanisHeight/150,-8*alanisHeight/150);
ellipse(bodyX+(100*alanisHeight/150),bodyY+(43*alanisHeight/150),90*alanisHeight/150,-23*alanisHeight/150);
fill(8, 6, 6);
};

var drawAlanis= function (alanisX,alanisY,alanisHeight){
    drawhead(alanisX,alanisY,alanisHeight);
    drawhair(alanisX,alanisY,alanisHeight);
    drawfeatures(alanisX,alanisY,alanisHeight);
    drawbody(alanisX,alanisY,alanisHeight);

};//bitmojiAlanis
