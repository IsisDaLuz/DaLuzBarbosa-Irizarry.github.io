    }
    else if (currentScene===10)
    {
        gameScene10();
    }
    else if (currentScene===11)
    {
        gameScene11();
    }
    else if (currentScene===12)
    {
        gameScene12();
    }
    else if (currentScene===13)
    {
        gameScene13();
    }
    else if (currentScene===14)
    { 
        gameScene14();
    }
    else if (currentScene===15)
