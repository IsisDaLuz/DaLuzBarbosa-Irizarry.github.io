var gameScene1 = function() {
    currentScene =1;
    background(173, 239, 255);
    image(getImage("cute/Star"),148,-14);
    stroke(0, 0, 0);
    fill(252, 252, 252);
    rect(20,149,358,66);
    fill(7, 14, 145);
    textSize(22);
    text("What is the rarest m&m color?",49, 170);
    fill(255, 255, 255);
    rect(4,231,184,45);// top right
    rect(4,300,184,45);// bottom right
    rect(210,300,184,45);//bottom left
    rect(210,231,184,45);// top left
    fill(7, 14, 145);
    text("Yellow", 54,242); 
    text("Brown",262,242);//right answer
    text("Orange",262,309);
    text("Red",71,309);
    textSize(22);
    text("Score: " + score, 294, 30);
    text("Time:" + round((millis()-startTime)/1000),13,30);
    drawBitmojiIsis(73,91,48);
    drawAlanis(270,50,57);
    fill(251, 255, 0);
    text("Isis",64,132);
    text("Alanis",296,133);
}; //first question

