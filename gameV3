draw = function() {
    if (currentScene === 0)
    {
        splashScreen();
    }
    
   else if (currentScene === 1)
    {
        gameScene1();
    }
    else if (currentScene === 2)
    {
        gameScene2();
        
    }
    else if (currentScene === 3)
    {
        gameScene3();
    }
    else if (currentScene ===4)
    { 
        gameScene4();
    }
    else if (currentScene=== 5)
    {
        gameScene5();
    }
    else if (currentScene===6)
